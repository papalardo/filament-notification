{
    "name": "webbingbrasil/filament-notification",
    "description": "A notification center package for filament.",
    "keywords": [
        "laravel",
        "filament",
        "filament-notification",
        "notification"
    ],
    "homepage": "https://github.com/webbingbrasil/filament-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Andrade",
            "email": "danilo@webbingbrasil.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.16",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Webbingbrasil\\FilamentNotification\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webbingbrasil\\FilamentNotification\\FilamentNotificationProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
